<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th>Tipo Telefono</th>
        <th>Telefono</th>
        <th>Ext</th>
        <th>Estatus</th>
    </tr>
    {{#each value}}
        <tr>
            <td>{{tipo_label}}</td>
            <td>{{telefono}}</td>
            <td>{{extension}}</td>
            <td>{{estatus}}</td>
        </tr>
    {{/each}}
</table>