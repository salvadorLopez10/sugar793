        position: fixed;
        z-index: 1000;
    }

    .backlogHeaderFloat_totals {
        position: fixed;
        z-index: 1000;
    }

    .innerTableTd {
        background: transparent !important;
        border: none !important;
    }

    .dashlet-content > div {
        overflow-x: scroll;
    }

    .dashboard .thumbnail.dashlet:not(.collapsed) {
        max-height: 100%;
        height: 100%;
